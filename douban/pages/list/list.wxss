.header {
  border-bottom: 1px solid #eee;
  padding: 10rpx;
  text-align: center;
}

.list {
  padding: 20rpx;
}

.list image {
  width: 150rpx;
  height: 200rpx;
  float: left;
  margin-right: 10rpx;
}
navigator{
	width: 100%;
	height: 230rpx;
	margin-top: 20rpx;
  border-bottom: 1px solid #eee;
}
.title{
	width: 400rpx;
}
text{
	display: block;
	overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.rating{
	float: right;
	margin-top: -150rpx;
	background-color: brown;
	color: white;
	border-radius: 16rpx;
	font-size: 30rpx;
	width: 50rpx;
	text-align: center;
}
